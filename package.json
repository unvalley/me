{
	"name": "unvalley-me",
	"scripts": {
		"start": "next dev",
		"dev": "next dev",
		"build": "pnpm run build:content && next build",
		"build:content": "contentlayer2 build",
		"postbuild": "pnpm run sitemap && pnpm run rss",
		"sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
		"rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
		"serve": "next start",
		"analyze": "cross-env ANALYZE=true next build",
		"lint": "biome lint",
		"lint:staged": "biome lint --staged",
		"lint:fix": "biome lint --write ./app ./components ./scripts ./lib",
		"lint:fix:unsafe": "biome lint --write --unsafe ./app ./components ./scripts ./lib",
		"format": "biome format --write ./app ./components ./scripts ./lib",
		"post:new": "node ./scripts/compose.js"
	},
	"dependencies": {
		"@fontsource/inter": "5.2.6",
		"@next/third-parties": "^15.3.5",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.21",
		"contentlayer2": "^0.5.8",
		"esbuild": "^0.25.5",
		"github-slugger": "^2.0.0",
		"image-size": "2.0.2",
		"js-yaml": "^4.1.0",
		"mdx-bundler": "^10.1.1",
		"next": "^15.3.5",
		"next-contentlayer2": "^0.5.8",
		"next-themes": "^0.4.6",
		"postcss": "^8.5.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"reading-time": "1.5.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-citation": "^2.3.1",
		"rehype-katex": "^7.0.1",
		"rehype-preset-minify": "^7.0.1",
		"rehype-prism-plus": "^2.0.1",
		"rehype-slug": "^6.0.0",
		"remark": "^15.0.1",
		"remark-gfm": "^4.0.1",
		"remark-math": "^6.0.0",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@next/bundle-analyzer": "15.3.5",
		"@svgr/webpack": "^8.1.0",
		"@types/mdx": "^2.0.13",
		"@types/react": "^19.1.8",
		"cross-env": "^7.0.3",
		"dedent": "^1.6.0",
		"globby": "14.1.0",
		"inquirer": "^12.7.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3"
	},
	"packageManager": "pnpm@9.15.1"
}
