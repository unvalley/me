{
	"name": "unvalley-me",
	"scripts": {
		"start": "next dev",
		"dev": "next dev",
		"build": "next build",
		"postbuild": "pnpm run sitemap && pnpm run rss",
		"sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
		"rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
		"serve": "next start",
		"analyze": "cross-env ANALYZE=true next build",
		"lint": "biome lint",
		"lint:staged": "biome lint --staged",
		"lint:fix": "biome lint --write ./pages ./components ./scripts ./lib",
		"lint:fix:unsafe": "biome lint --write --unsafe ./pages ./components ./scripts ./lib",
		"format": "biome format --write ./pages ./components ./scripts ./lib",
		"post:new": "node ./scripts/compose.js"
	},
	"dependencies": {
		"@fontsource/inter": "5.0.20",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"autoprefixer": "^10.4.20",
		"contentlayer": "^0.2.9",
		"github-slugger": "^1.3.0",
		"image-size": "1.0.0",
		"js-yaml": "^4.1.0",
		"mdx-bundler": "^8.0.0",
		"next": "^13.1.6",
		"next-contentlayer": "^0.2.9",
		"next-themes": "^0.2.1",
		"postcss": "^8.4.45",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"reading-time": "1.3.0",
		"rehype-autolink-headings": "6.1.0",
		"rehype-citation": "^0.4.0",
		"rehype-katex": "^6.0.2",
		"rehype-preset-minify": "6.0.0",
		"rehype-prism-plus": "^1.1.3",
		"rehype-slug": "^5.0.0",
		"remark": "^14.0.2",
		"remark-footnotes": "^4.0.1",
		"remark-gfm": "^3.0.1",
		"remark-math": "^5.1.1",
		"tailwindcss": "^3.4.10",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@next/bundle-analyzer": "14.2.8",
		"@svgr/webpack": "^6.1.2",
		"@types/react": "^18.0.27",
		"cross-env": "^7.0.3",
		"dedent": "^1.5.3",
		"globby": "14.0.2",
		"inquirer": "^10.2.1",
		"typescript": "^5.5.4"
	},
	"packageManager": "pnpm@9.9.0"
}
