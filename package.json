{
	"name": "unvalley-me",
	"scripts": {
		"start": "next dev",
		"dev": "next dev",
		"build": "next build",
		"postbuild": "pnpm run sitemap && pnpm run rss",
		"sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
		"rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
		"serve": "next start",
		"analyze": "cross-env ANALYZE=true next build",
		"lint": "biome lint",
		"lint:staged": "biome lint --staged",
		"lint:fix": "biome lint --write ./app ./components ./scripts ./lib",
		"lint:fix:unsafe": "biome lint --write --unsafe ./app ./components ./scripts ./lib",
		"format": "biome format --write ./app ./components ./scripts ./lib",
		"post:new": "node ./scripts/compose.js"
	},
	"dependencies": {
		"@fontsource/inter": "5.2.6",
		"@mdx-js/loader": "^3.0.1",
		"@mdx-js/react": "^3.0.1",
		"@next/mdx": "^14.1.4",
		"@next/third-parties": "^15.3.5",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.21",
		"clsx": "^2.1.0",
		"github-slugger": "^2.0.0",
		"gray-matter": "^4.0.3",
		"image-size": "2.0.2",
		"katex": "^0.16.11",
		"next": "^15.3.5",
		"next-themes": "^0.4.6",
		"postcss": "^8.5.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"shiki": "^3.2.1",
		"tailwindcss": "^3.4.17"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@next/bundle-analyzer": "15.3.5",
		"@svgr/webpack": "^8.1.0",
		"@types/mdx": "^2.0.13",
		"@types/react": "^19.1.8",
		"cross-env": "^7.0.3",
		"dedent": "^1.6.0",
		"globby": "14.1.0",
		"inquirer": "^12.7.0",
		"typescript": "^5.8.3"
	},
	"packageManager": "pnpm@9.15.1"
}
